#sama dengan 
a = 10
b = 5
if  a == b:
	print("itu salah")
#tidak sama dengan
if a != b:
	print("itu benar")
#lebih besar dari
if a > b :
	print ("itu benar")
#lebih kecil dari
if a < b :
	print ("itu benar")
#lebih besar atau sama dengan
if a >= b :
	print ("itu benar")
#lebih kecil atau sama dengan
if a <= b :
	print("itu salah")

